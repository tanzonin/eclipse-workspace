import DiamonShop.Entity.Slides;

@Repository
public class HomeDao {
	@Autowired
	JdbcTemplate _jdbcTemplate;
	
	public List<Slides> GetDataSlide(){
		List<Slides> list = new ArrayList<Slides>();
		
		return list;
	}
}
